{
  // 插件清单文件的版本，这个必须写，而且必须是2
  "manifest_version": 2,
  // 插件的名称
  "name": "Amazon Scripts",
  // 插件的版本
  "version": "2020.10.01",
  // 插件描述
  "description": "Amazon Scripts",
  // 图标，一般偷懒全部用一个尺寸的也没问题
  "icons": {
    "48": "icons/amazon_scripts_chrome_48.png"
  },
  // 插件主页，这个很重要，不要浪费了这个免费广告位
  "homepage_url": "https://www.baidu.com",
  // 会一直常驻的后台JS或后台页面
  "background": {
    // 2种指定方式，如果指定JS，那么会自动生成一个背景页
    // "page": "background.html"
    "scripts": ["scripts/background.js"]
  },
  // 浏览器右上角图标设置，browser_action、page_action、app必须三选一
  "browser_action": {
    "browser_style": true,
    // 图标悬停时的标题，可选
    "default_title": "Amazon Scripts",
    // 浏览器右上角默认的页面
    "default_popup": "popup.html",
    // 浏览器右上角默认的图标
    "default_icon": "icons/amazon_scripts_chrome_48.png"
  },
  // 当某些特定页面打开才显示的图标
  /*"page_action":
	{
		"default_icon": "img/icon.png",
		"default_title": "我是pageAction",
		"default_popup": "popup.html"
	},*/
  // 权限申请
  "permissions": [
    // "contextMenus", // 右键菜单
    // "tabs", // 标签
    // "notifications", // 通知
    "webRequest", // web请求
    "webRequestBlocking",
    // "storage", // 插件本地存储
    // "http://*/*", 可以通过executeScript或者insertCSS访问的网站
    // "https://*/*", // 可以通过executeScript或者insertCSS访问的网站
    "*://www.amazon.com/*",
    "*://www.amazon.ca/*",
    "*://www.amazon.com.mx/*",
    "*://www.amazon.co.uk/*",
    "*://www.amazon.de/*",
    "*://www.amazon.es/*",
    "*://www.amazon.fr/*",
    "*://www.amazon.it/*",
    "*://www.amazon.co.jp/*",
    "*://www.amazon.com.au/*",
    "*://members.helium10.com/*",
    "*://*.aliexpress.com/*",
    "*://*.ecomtool.net/*",
    "*://translate.google.cn/*"
  ],
  // 普通页面能够直接访问的插件资源列表，如果不设置是无法直接访问的
  "web_accessible_resources": [
    "template/amazom_asin_sale.html",
    "template/amazon_keyword_rank.html",
    "template/amazon_check_keyword_rank.html",
    "template/amazon_rank_index.html",
    "template/amazon_check_rank_index.html",
    "template/prodDetails.html",
    "scripts/amazon_scripts_page.js",
    "style/amazon_scripts.css"
  ],
  // 需要直接注入页面的JS
  "content_scripts": [
    {
      //"matches": ["http://*/*", "https://*/*"],
      // "<all_urls>" 表示匹配所有地址
      "matches": [
        "*://sellersprite.com/favicon.ico*",
        "*://www.sellersprite.com/favicon.ico*",
        "*://www.sellersprite.com/v2/keyword/history-trend-month*",
        "*://members.helium10.com/favicon.ico*",
        "*://www.amazon.com/empty*",
        "*://www.amazon.ca/empty*",
        "*://www.amazon.com.mx/empty*",
        "*://www.amazon.co.uk/empty*",
        "*://www.amazon.de/empty*",
        "*://www.amazon.es/empty*",
        "*://www.amazon.fr/empty*",
        "*://www.amazon.it/empty*",
        "*://www.amazon.co.jp/empty*",
        "*://www.amazon.com.au/empty*",
        "*://www.amazon.com/favicon.ico*",
        "*://www.amazon.ca/favicon.ico*",
        "*://www.amazon.com.mx/favicon.ico*",
        "*://www.amazon.co.uk/favicon.ico*",
        "*://www.amazon.de/favicon.ico*",
        "*://www.amazon.es/favicon.ico*",
        "*://www.amazon.fr/favicon.ico*",
        "*://www.amazon.it/favicon.ico*",
        "*://www.amazon.co.jp/favicon.ico*",
        "*://www.amazon.com.au/favicon.ico*",
        "*://www.amazon.com/*",
        "*://www.amazon.ca/*",
        "*://www.amazon.com.mx/*",
        "*://www.amazon.co.uk/*",
        "*://www.amazon.de/*",
        "*://www.amazon.es/*",
        "*://www.amazon.fr/*",
        "*://www.amazon.it/*",
        "*://www.amazon.co.jp/*",
        "*://www.amazon.com.au/*",
        "*://completion.amazon.com/*",
        "*://completion.amazon.co.uk/*",
        "*://completion.amazon.co.jp/*",
        "*://www.google.com/favicon.ico*",
        "*://translate.google.cn/favicon.ico*",
        "*://js.amazonwords.cn/*"
      ],
      // 多个JS按顺序注入
      "js": ["scripts/amazon_navigation.js", "scripts/amazon_scripts.js"],
      // JS的注入可以随便一点，但是CSS的注意就要千万小心了，因为一不小心就可能影响全局样式
      // "css": ["css/custom.css"],
      // 代码注入的时间，可选值： "document_start", "document_end", or "document_idle"，最后一个表示页面空闲时，默认document_idle
      "run_at": "document_end"
    }
  ]
}
